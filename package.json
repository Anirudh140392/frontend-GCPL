{
  "name": "frontend-unified",
  "version": "1.0.0",
  "description": "Unified frontend monorepo for GCPL, Samsonite, Bowlers, and Bunge clients",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gcpl": "VITE_CLIENT=gcpl vite build",
    "build:samsonite": "VITE_CLIENT=samsonite vite build",
    "build:bowlers": "VITE_CLIENT=bowlers vite build",
    "build:bunge": "VITE_CLIENT=bunge vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "clean": "rm -rf dist node_modules/.vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/x-data-grid": "^6.18.2",
    "@mui/x-date-pickers": "^6.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "date-fns": "^2.30.0",
    "recharts": "^2.8.0",
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^5.0.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jest": "^29.7.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "^5.2.2",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "vite",
    "monorepo",
    "multi-client",
    "analytics",
    "dashboard",
    "gcpl",
    "samsonite",
    "bowlers",
    "bunge"
  ],
  "author": "Trailytics Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Anirudh140392/frontend-unified.git"
  },
  "bugs": {
    "url": "https://github.com/Anirudh140392/frontend-unified/issues"
  },
  "homepage": "https://github.com/Anirudh140392/frontend-unified#readme"
}

